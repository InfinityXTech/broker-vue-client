{
    "type": "Datatable",
    "title": "Advertisers",
    "columns": {
        "_id": {
            "label": "#",
            "sortable": false,
            "renderCell": "simple.Index"
        },
        "final_status_changed_date": {
            "label": "Timestamp",
            "renderCell": "simple.DateTime"
        },
        "broker_data": {
            "label": "Advertiser",
            "renderCell": "custom.AdvertiserName"
        },
        "transaction_id": {
            "label": "Transaction ID"
        },
        "final_status_date_pay": {
            "label": "Pay Timestamp",
            "renderCell": "simple.Date"
        },
        "total": {
            "label": "Total",
            "default": 0,
            "renderCell": "simple.MoneyColor",
            "totalCalc": "simple.Sum"
        },
        "final_status_changed_user.name": {
            "label": "Approved by"
        }
    }
}