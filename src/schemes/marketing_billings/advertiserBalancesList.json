{
    "type": "Datatable",
    "title": "Advertisers",
    "columns": {
        "_id": {
            "label": "#",
            "sortable": false,
            "renderCell": "simple.Index"
        },
        "name": {
            "label": "Advertiser",
            "renderCell": "custom.AdvertiserName"
        },
        "is_collection": {
            "label": "Collection",
            "renderCell": "custom.Collection"
        },
        "status": {
            "label": "Status",
            "renderCell": "custom.Status"
        },
        "account_manager.account_email": {
            "label": "Account Manager"
        },
        "balance": {
            "label": "Balance",
            "default": 0,
            "renderCell": "simple.MoneyColor",
            "totalCalc": "simple.Sum"
        }
    }
}