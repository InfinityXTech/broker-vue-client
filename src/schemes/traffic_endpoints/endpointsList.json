{
    "type": "Datatable",
    "title": "Traffic Endpoints",
    "uri": "/traffic_endpoint/all",
    "columns": {
        "_id": {
            "label": "ID",
            "sortable": false,
            "styles": [
                {
                    "textAlign": "center"
                }
            ],
            "renderCell": "simple.Index"
        },
        "status": {
            "label": "Status",
            "sortable": false,
            "renderCell": "simple.StatusTag({1: {color: 'primary', title: 'ACTIVE'}, 0: {color: 'warning', title: 'INACTIVE'}, 2: {color: 'grey', title: 'ARCHIVED'}})"
        },
        "tag_management": {
            "label": "Tags",
            "sortable": false,
            "renderCell": "custom.Tags"
        },
        "token": {
            "label": "Traffic Endpoint",
            "renderCell": "custom.Token"
        },
        "today_leads": {
            "label": "Today Leads",
            "default": 0
        },
        "today_deposits": {
            "label": "Today Deposits",
            "default": 0
        },
        "today_cr": {
            "label": "Today CR",
            "default": 0
        },
        "total_leads": {
            "label": "Total Leads",
            "default": 0
        },
        "total_deposits": {
            "label": "Total Deposits",
            "default": 0
        },
        "total_cr": {
            "label": "Total CR",
            "default": 0
        },
        "account_manager_user.name": {
            "label": "Account Manager"
        }
    }
}