{
    "type": "Datatable",
    "title": "Brokers",
    "columns": {
        "_id": {
            "label": "ID",
            "sortable": false,
            "styles": [
                {
                    "textAlign": "center"
                }
            ],
            "renderCell": "simple.Index"
        },
        "status": {
            "label": "Status",
            "sortable": false,
            "renderCell": "simple.StatusTag({1: {color: 'primary', title: 'ACTIVE'}, 0: {color: 'warning', title: 'INACTIVE'}, 2: {color: 'grey', title: 'ARCHIVED'}})"
        },
        "partner_name": {
            "label": "Broker",
            "renderCell": "custom.BrokerName"
        },
        "today_leads": {
            "label": "Today Leads",
            "default": 0
        },
        "today_ftd": {
            "label": "Today FTD's",
            "default": 0
        },
        "today_cr": {
            "label": "Today CR",
            "default": 0
        },
        "total_leads": {
            "label": "Total Leads",
            "default": 0
        },
        "total_ftd": {
            "label": "Total FTD's",
            "default": 0
        },
        "total_cr": {
            "label": "Total CR",
            "default": 0,
            "renderCell": "simple.Percent"
        },
        "account_manager_user": {
            "label": "Account Manager",
            "renderCell": "custom.AccountName"
        }
    }
}