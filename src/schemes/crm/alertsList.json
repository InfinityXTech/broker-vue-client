{
    "type": "Datatable",
    "title": "Brokers",
    "columns": {
        "_id": {
            "label": "ID",
            "sortable": false,
            "styles": [
                {
                    "textAlign": "center"
                }
            ],
            "renderCell": "simple.Index"
        },
        "status": {
            "label": "Status",
            "sortable": false,
            "renderCell": "simple.StatusTag({1: {color: 'primary', title: 'Active and waiting for execution'}, 2: {color: 'lime', title: 'Executed'}, 3: {color: 'danger', title: 'Deleted'}})"
        },
        "user.name": {
            "label": "Created By"
        },
        "execution_at": {
            "label": "Execution At",
            "renderCell": "simple.DateTime"
        },
        "lead.broker_data.partner_name_secure": {
            "label": "Broker"
        },
        "lead.endpoint_data.token": {
            "label": "Endpoint"
        },
        "time.left": {
            "label": "Time left"
        },
        "actions": {
            "label": "Actions",
            "sortable": false,
            "renderCell": "custom.Actions"
        }
    }
}