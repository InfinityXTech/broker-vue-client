{
    "type": "Datatable",
    "title": "Chargebacks",
    "columns": {
        "_id": {
            "label": "#",
            "sortable": false,
            "renderCell": "simple.Index"
        },
        "created_at": {
            "label": "Timestamp",
            "renderCell": "simple.DateTime"
        },
        "payment_method_data": {
            "label": "Payment method",
            "renderCell": "custom.PaymentMethod"
        },
        "payment_request_data.total": {
            "label": "Payment request",
            "renderCell": "simple.MoneyColor"
        },
        "status": {
            "label": "Status",
            "sortable": false,
            "renderCell": "custom.Status"
        },
        "final_status_date_pay": {
            "label": "Pay Timestamp",
            "sortable": false,
            "renderCell": "simple.Date"
        },
        "transaction_id": {
            "label": "Transaction ID"
        },
        "amount": {
            "label": "Sum",
            "renderCell": "simple.MoneyColor"
        },
        "screenshots": {
            "label": "Screenshots",
            "default": [],
            "renderCell": "custom.Files"
        },
        "actions": {
            "label": "Actions",
            "sortable": false,
            "renderCell": "custom.Actions"
        }
    }
}